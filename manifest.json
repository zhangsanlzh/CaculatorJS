{
  "manifest_version": 2,

  "name": "Easy Caculator",
  "description": "This extension help you to do some math caculation",
  "version": "1.0",

  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html",
    "default_title": "Easy Caculator"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],

  "background":
  {
    "scripts":["js/libs/jquery_min.js"]
  }

